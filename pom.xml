<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <!-- Parent POM of plugin -->
    <parent>
        <groupId>org.jenkins-ci.plugins</groupId>
        <artifactId>plugin</artifactId>
        <version>2.33</version>
        <relativePath />
    </parent>
    <artifactId>pureload</artifactId>
    <version>1.10-SNAPSHOT</version>
    <packaging>hpi</packaging>
    <properties>
        <!-- The Jenkins version required by the plugin -->
        <jenkins.version>1.651.3</jenkins.version>

        <workflow-step-api-plugin.version>2.13</workflow-step-api-plugin.version>
        <!-- The Java version to use to build the plugin.
        Should match the minimum Java version for the selected Jenkins version -->
        <java.level>7</java.level>
    </properties>
    <name>PureLoad Plugin</name>
    <description>Publish load test results produced by PureLoad .</description>
    <url>https://external.pureload.com/jenkins/PureLoad+Plugin.html</url>

    <!-- License -->
    <licenses>
        <license>
            <name>MIT License</name>
            <url>http://opensource.org/licenses/MIT</url>
        </license>
    </licenses>

    <developers>
      <developer>
        <id>JanAndersson</id>
        <name>Jan Andersson</name>
        <email>jan.andersson@pureload.com</email>
      </developer>
    </developers>

    <!-- Source hosted  on @jenkinsci: -->
    <scm>
        <connection>scm:git:git://github.com/jenkinsci/${project.artifactId}-plugin.git</connection>
        <developerConnection>scm:git:git@github.com:jenkinsci/${project.artifactId}-plugin.git</developerConnection>
        <url>https://github.com/jenkinsci/${project.artifactId}-plugin</url>
      <tag>HEAD</tag>
  </scm>

